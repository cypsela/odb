{
  "name": "opal",
  "version": "0.0.1",
  "description": "peer-to-peer, collaborative states using merkle-crdts",
  "main": "src/index.js",
  "type": "module",
  "prettier": "prettier-config-standard",
  "scripts": {
    "postinstall": "npm run exports-keychain",
    "lint": "eslint src test",
    "format": "prettier -w src test && eslint --fix src test",
    "exports-keychain": "json -I -f ./node_modules/libp2p/package.json -e 'this.exports[\"./keychain\"] = { \"types\": \"./dist/src/keychain/index.d.ts\", \"import\": \"./dist/src/keychain/index.js\" }'",
    "build": "tsc",
    "test": "npm run clean && c8 -c ./test/.c8rc.json npx mocha --config ./test/.mocharc.json ; npm run clean",
    "clean": "git add test/fixtures && git stash -- test/fixtures && git stash drop ; rm -rf test/temp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cypsela/odb.git"
  },
  "keywords": [
    "peer-to-peer",
    "ipfs",
    "ipld",
    "merkle-crdt"
  ],
  "author": "tabcat <tabcat00@proton.me>",
  "bugs": {
    "url": "https://github.com/cypsela/odb/issues"
  },
  "homepage": "https://github.com/cypsela/odb#readme",
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "c8": "^7.12.0",
    "eslint": "^8.23.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "ipfs": "^0.65.0",
    "json": "^11.0.0",
    "mocha": "^9.2.2",
    "polendina": "^3.1.0",
    "prettier": "^2.7.1",
    "prettier-config-standard": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^7.0.2",
    "@libp2p/crypto": "^1.0.4",
    "@libp2p/interface-keys": "^1.0.3",
    "@libp2p/interface-pubsub": "^2.0.1",
    "@libp2p/interfaces": "^3.0.3",
    "datastore-level": "^9.0.1",
    "interface-datastore": "^7.0.0",
    "ipfs-core-types": "^0.12.1",
    "ipld-hashmap": "^2.1.12",
    "it-all": "^1.0.6",
    "libp2p": "^0.40.0",
    "make-dir": "^3.1.0",
    "multiformats": "^9.7.0",
    "uint8arrays": "^3.0.0",
    "wherearewe": "^1.0.2"
  }
}
